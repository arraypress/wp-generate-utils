{
  "name": "arraypress/wp-generate-utils",
  "version": "1.0.0",
  "description": "A lean WordPress library for generating unique identifiers, professional codes, slugs, and secure tokens.",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/arraypress/wp-generate-utils",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\GenerateUtils\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/wp-generate-utils/issues",
    "source": "https://github.com/arraypress/wp-generate-utils"
  },
  "keywords": [
    "wordpress",
    "wp",
    "uuid",
    "token",
    "code-generator",
    "slug",
    "unique",
    "identifier",
    "secure",
    "random",
    "magic-link",
    "arraypress"
  ]
}